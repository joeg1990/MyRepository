using System;
using System.Collections;
using System.Collections.Generic;

namespace Cake_Code
{
    class Program
    {
        static void Main(string[] args)
        {
            CakeValues<string,string> myCake = new CakeValues<string,string>();

            myCake.MyCakeType = "Wedding Cake";
            myCake.MyCakeFlavor = " is a Vallina Cake";
            Console.WriteLine(myCake.MyCakeType + "," + myCake.MyCakeFlavor);

            CakeValues<string,string> myCake2 = new CakeValues<string,string>();

            myCake2.MyCakeType = "Party Cake";
            myCake2.MyCakeFlavor = " is a Chocolate Cake";
            Console.WriteLine(myCake2.MyCakeType + "," + myCake2.MyCakeFlavor);
        }
    }
}
 public class CakeValues<Cake,Taste>
{
   public Cake MyCakeType { get; set; }
    public Taste MyCakeFlavor { get; set; }
 
}
