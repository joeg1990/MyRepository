using System;
using System.Collections;
using System.Collections.Generic;

namespace Cake_Code
{
    class Program
    {
        static void Main(string[] args)
        {
            MyFirstCake myCake = new MyFirstCake();
            myCake.MyType<string>("Wedding Cake", "Vanilla");


            MysecondCake myCake2 = new MysecondCake();
            myCake2.MyType2<string>("Party Cake", "Chocolate");

        }
    }
}
public class MyFirstCake
{
  public void MyType<T>(T myCakeType, string myFlavor)
    {
        Console.WriteLine("My first Cake is:" + myCakeType);
        Console.WriteLine("My frist Cake flavor is:" + myFlavor);
    }
}
public class MysecondCake
{
    public void MyType2<T2>(T2 myCakeType2, string myFlavor2)
    {
        Console.WriteLine("My second Cake is:" + myCakeType2);
        Console.WriteLine("My second Cake flavor is:" + myFlavor2);
    }
}
